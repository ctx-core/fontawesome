{
	"name": "@ctx-core/fontawesome",
	"version": "15.0.55",
	"description": "ctx-core fontawesome",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/fontawesome#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"bin": {
		"refresh-font-awesome": "./bin/refresh-font-awesome.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.21",
		"@ctx-core/svg": "workspace:^13.0.72",
		"domutils": "^2.8.0",
		"esm": "^3.2.25",
		"globby": "^12.0.2",
		"htmlparser2": "^7.1.2",
		"import-meta-resolve": "^1.1.1",
		"svelte": "3.43.0"
	},
	"devDependencies": {
		"@ctx-core/cli-args": "workspace:^10.0.21",
		"typescript": "^4.4.3"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}