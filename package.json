{
	"name": "@ctx-core/fontawesome",
	"version": "16.2.247",
	"type": "module",
	"description": "ctx-core fontawesome",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/fontawesome#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fontawesome.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-fontawesome@briantakita.me"
	},
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"icon_name_svg"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@ctx-core/svg-ui-svelte": "^1.3.251"
	},
	"devDependencies": {
		"c8": "^10.1.2",
		"check-dts": "^0.8.0",
		"tsx": "^4.16.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}