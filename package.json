{
	"name": "@ctx-core/fontawesome",
	"version": "14.0.1",
	"description": "ctx-core fontawesome",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/fontawesome#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"dependencies": {
		"@ctx-core/array": "workspace:^22.0.1",
		"@ctx-core/svg": "workspace:^12.0.2",
		"domutils": "^2.7.0",
		"esm": "^3.2.25",
		"globby": "^11.0.3",
		"htmlparser2": "^6.1.0",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/cli-args": "workspace:^9.0.1",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
