{
	"name": "@ctx-core/fontawesome",
	"version": "16.0.13",
	"description": "ctx-core fontawesome",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/fontawesome#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fontawesome.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"types": "./lib/index.d.ts",
	"exports": {
		".": {
			"import": "./lib/index.js"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"@ctx-core/svg-ui-svelte": "workspace:^1.1.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.125",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"type": "module",
	"svelte": "./lib/index.js"
}
