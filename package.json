{
	"name": "@ctx-core/fontawesome",
	"version": "15.1.9",
	"description": "ctx-core fontawesome",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/fontawesome#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/fontawesome/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"bin": {
		"refresh-font-awesome": "./bin/refresh-font-awesome.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.30",
		"@ctx-core/svg": "workspace:^14.0.6",
		"domutils": "^2.8.0",
		"globby": "^12.0.2",
		"htmlparser2": "^7.2.0",
		"import-meta-resolve": "^1.1.1",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/cli-args": "workspace:^11.0.7",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}